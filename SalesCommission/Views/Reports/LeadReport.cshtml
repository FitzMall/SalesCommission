@model SalesCommission.Models.LeadReportModel

@{
    ViewBag.Title = "Lead Report";
    var applicationPath = Request.ApplicationPath;

    if (applicationPath == "/")
    {
        applicationPath = "";
    }

    var storeLocations = new List<string>();

    if (Model.StoreLeadInformation != null)
    {
        foreach (var store in Model.StoreLeadInformation)
        {
            var storeName = store.StoreId;

            foreach (var vinStore in SalesCommission.Business.Enums.VinStores)
            {
                if (vinStore.StoreId == store.DealerId)
                {
                    storeName = vinStore.Name;
                }
            }
            storeLocations.Add(storeName);
        }
    }

    var brands = new List<string>();

    brands.Add("Buick");
    brands.Add("Cadillac");
    brands.Add("Chevrolet");
    brands.Add("Chrysler");
    brands.Add("Dodge");
    brands.Add("Genesis");
    brands.Add("GMC");
    brands.Add("Hyundai");
    brands.Add("Jeep");
    brands.Add("Mazda");
    brands.Add("Mitsubishi");
    brands.Add("Nissan");
    brands.Add("RAM");
    brands.Add("Subaru");
    brands.Add("Toyota");
    brands.Add("Volkswagen");
    brands.Add("Used");
    //brands.Add("Handyman");
    brands.Add("Other");


    var associates = new List<string>();
    var leadSources = new List<string>();
    var leadGroups = new List<string>();

    var leadStatus = new List<string>();
    var leadConditions = new List<string>();
    var salesTeams = new List<string>();

    if (Model.AssociateLeads != null)
    {
        associates = Model.AssociateLeads.Select(x => x.Sales_LastName).Distinct().OrderBy(x => x).ToList();
        leadSources = Model.AssociateLeads.Select(x => x.LeadSourceName).Distinct().OrderBy(x => x).ToList();
        leadGroups = Model.AssociateLeads.Select(x => x.LeadGroupMapping).Distinct().OrderBy(x => x).ToList();
        leadStatus = Model.AssociateLeads.Select(x => x.LeadStatusTypeName).Distinct().OrderBy(x => x).ToList();
        leadConditions = Model.AssociateLeads.Select(x => x.LeadStatusName).Distinct().OrderBy(x => x).ToList();
        salesTeams = Model.AssociateLeads.Select(x => x.Team).Distinct().OrderBy(x => x).ToList();
    }

    var breakDowns = new List<string>();
    var breakDownValues = new List<string>();

    breakDowns.Add("Associate");
    breakDownValues.Add("associatename");

    breakDowns.Add("Brand");
    breakDownValues.Add("brand");

    breakDowns.Add("Inventory Type");
    breakDownValues.Add("inventorytype");

    breakDowns.Add("Lead Date");
    breakDownValues.Add("leaddate");

    breakDowns.Add("Lead Time");
    breakDownValues.Add("leadtime");

    breakDowns.Add("Lead Group");
    breakDownValues.Add("leadgroupname");

    breakDowns.Add("Lead Source");
    breakDownValues.Add("leadsourcename");

    breakDowns.Add("Lead Condition");
    breakDownValues.Add("leadstatusname");

    breakDowns.Add("Lead Status");
    breakDownValues.Add("leadstatustype");

    breakDowns.Add("Location");
    breakDownValues.Add("locationid");

    breakDowns.Add("Make");
    breakDownValues.Add("make");

    breakDowns.Add("Model");
    breakDownValues.Add("model");

    breakDowns.Add("Sales Team");
    breakDownValues.Add("salesteam");

    breakDowns.Add("Stock Number");
    breakDownValues.Add("stock");

    breakDowns.Add("Zip Code");
    breakDownValues.Add("zip");

}
