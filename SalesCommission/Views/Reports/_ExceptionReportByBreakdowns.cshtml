<div class="col-sm-12" id="location-associate">
    <h4> Report Results for @Model.ReportStartMonth/@Model.ReportStartYear to @Model.ReportEndMonth/@Model.ReportEndYear</h4>

    <table class="table table-bordered" style="font-size:12px">
            <thead>

                <tr style="background-color:#d9edf7;">
                    <th class="text-center">Company Totals</th>
                    <th class="text-center">Deals</th>
                    <th class="text-center">Rate Exceptions</th>
                    <th class="text-center">Rate Exception %</th>
                    @*<th class="text-center">Price Exceptions</th>
                    <th class="text-center">Price Exception %</th>*@
                </tr>

            </thead>
            <tbody>

                @{
                    var TotalDealCount = 0;
                    var TotalRateExceptionCount = 0;
                    var TotalPriceExceptionCount = 0;

                    foreach (var detail in Model.ExceptionDetails)
                    {
                        TotalDealCount += 1;

                        if(detail.sl_rate_exception != null && detail.sl_rate_exception != "")
                        {
                            TotalRateExceptionCount += 1;
                        }

                        if (detail.sl_price_variance_exception != null && detail.sl_price_variance_exception != "")
                        {
                            TotalPriceExceptionCount += 1;
                        }
                    }

                    decimal TotalPriceExceptionPercent = 0;
                    decimal TotalRateExceptionPercent = 0;

                    if (TotalDealCount > 0)
                    {
                        TotalPriceExceptionPercent = ((decimal)TotalPriceExceptionCount / (decimal)TotalDealCount);
                        TotalRateExceptionPercent = ((decimal)TotalRateExceptionCount / (decimal)TotalDealCount);
                    }

                }        
                <tr>
                    <td class="text-center"></td>
                    <td class="text-center">@TotalDealCount</td>
                    <td class="text-center">@TotalRateExceptionCount</td>
                    <td class="text-center">@TotalRateExceptionPercent.ToString("P",nfi)</td>
                    @*<td class="text-center">@TotalPriceExceptionCount</td>
                    <td class="text-center">@TotalPriceExceptionPercent.ToString("P",nfi)</td>*@
                </tr>
            </tbody>
            <tfoot>
                <tr style="background-color:#d9edf7;">
                </tr>

            </tfoot>
        </table>
